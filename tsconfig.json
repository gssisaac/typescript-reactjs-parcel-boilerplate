{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "lib": ["es2017", "dom"],
    "baseUrl": ".",
    "types": ["node"]
  }
}
